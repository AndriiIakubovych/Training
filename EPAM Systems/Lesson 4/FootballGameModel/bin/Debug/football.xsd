<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="membership">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="teams">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="team" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="players">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="player" minOccurs="11" maxOccurs="11">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="name" type="xs:string"/>
                                <xs:element name="age" type="xs:integer"/>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>                         
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="coach" type="xs:string"/>                    
                  </xs:sequence>
                  <xs:attribute name="name" type="xs:string" use="required"/>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="referees">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="referee" type="xs:string" maxOccurs="unbounded"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>      
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>